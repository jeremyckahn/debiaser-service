{
  "name": "debiaser-service",
  "version": "0.0.0",
  "description": "A simple server-backed news-api proxy",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "deploy": "now --public -e NEWS_API_KEY=$(cat news-api-key.txt)",
    "undeploy": "now rm debiaser-service",
    "print:logs": "now logs $(now ls debiaser-service | tail -n2 | grep -o \"debiaser-service-.*.now.sh\")"
  },
  "author": "Jeremy Kahn <jeremyckahn@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "now": "^9.0.1"
  }
}
